{
	"name": "k-tokitoh-blog",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:playwright": "playwright test -c ./playwright-config/default.ts",
		"test:playwright:ci": "playwright test -c ./playwright-config/ci.ts",
		"test": "npm run test:unit run && npm run test:playwright",
		"test:ci": "npm run test:unit run && npm run test:playwright:ci",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^3.13.1",
		"mdsvex": "^0.12.6",
		"npm-check-updates": "^19.2.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"remark-abbr": "^1.4.1",
		"remark-github": "^11.2.4",
		"svelte": "^5.44.1",
		"svelte-check": "^3.8.6",
		"tslib": "^2.4.1",
		"typescript": "^5.9.3",
		"vite": "^6.4.1",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
